<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.org.dqgb.entity">

	<class name="AlarmType" table="T_ALARM_TYPE" lazy="true">

		<id name="id" column="PK_ID">
			<generator class="increment" />	
		</id>
		<property name="url" type="string" length="100" column="ACTION_NAME" />
		<property name="description" type="string" length="100" column="DESCRIPTION" />
	</class>
</hibernate-mapping>